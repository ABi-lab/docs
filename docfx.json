{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "ext/ImageSharp/src/**.csproj"
          ]
        }
      ],
      "dest": "api/ImageSharp",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      }
    },
    {
      "src": [
        {
          "files": [
            "ext/ImageSharp.Drawing/src/**.csproj"
          ]
        }
      ],
      "dest": "api/ImageSharp.Drawing",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      }
    },
    {
      "src": [
        {
          "files": [
            "ext/Fonts/src/SixLabors.Fonts/**.csproj"
          ]
        }
      ],
      "dest": "api/Fonts",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      }
    },
    {
      "src": [
        {
          "files": [
            "ext/ImageSharp.Web/src/ImageSharp.Web/**.csproj"
          ]
        }
      ],
      "dest": "api/ImageSharp.Web",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      }
    },
    {
      "src": [
        {
          "files": [
            "ext/ImageSharp.Web/src/ImageSharp.Web.Providers.Azure/**.csproj"
          ]
        }
      ],
      "dest": "api/ImageSharp.Web.Providers.Azure",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      }
    }
  ],
  "build": {
    "xref": [
      "xrefmap.yml"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**",
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          "CNAME"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "fileMetadata": {},
    "globalMetadata": {
      "_enableSearch": true,
      "_gitContribute": {
        "branch": "master"
      }
    },
    "template": [
      "statictoc",
      "templates"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}